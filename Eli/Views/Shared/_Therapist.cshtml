@model Eli.Models.tblTherapist



<!-- Modal -->
<div class="modal fade" id="@Model.ID" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                <h4 align="right " class="modal-title" id="myModalLabel"></h4>
            </div>
            <div class="modal-body" style="text-align:right">
                <h2>פרטי מטפל</h2>

                <fieldset>

                    <div class="display-label">
                        @Html.LabelFor(model => model.TherapistFirstName)
                    </div>
                    <div class="display-field">
                        @Html.DisplayFor(model => model.TherapistFirstName)
                    </div>

                    <div class="display-label">
                        @Html.LabelFor(model => model.TherapistSurName)
                    </div>
                    <div class="display-field">
                        @Html.DisplayFor(model => model.TherapistSurName)
                    </div>

                    <div class="display-label">
                        @Html.LabelFor(model => model.BirthDate)
                    </div>
                    <div class="display-field">
                        @Html.DisplayFor(model => model.BirthDate)
                    </div>

                    <div class="display-label">
                        @Html.LabelFor(model => model.Gender)
                    </div>
                    <div class="display-field">
                        @Html.DisplayFor(model => model.Gender)
                    </div>

                    <div class="display-label">
                        @Html.LabelFor(model => model.Address)
                    </div>
                    <div class="display-field">
                        @Html.DisplayFor(model => model.Address)
                    </div>

                    <div class="display-label">
                        @Html.LabelFor(model => model.ContcatPhoneNumber)
                    </div>
                    <div class="display-field">
                        @Html.DisplayFor(model => model.ContcatPhoneNumber)
                    </div>

                    <div class="display-label">
                        @Html.LabelFor(model => model.ContactMail)
                    </div>
                    <div class="display-field">
                        @Html.DisplayFor(model => model.ContactMail)
                    </div>

                    <div class="display-label">
                        @Html.LabelFor(model => model.UserName)
                    </div>
                    <div class="display-field">
                        @Html.DisplayFor(model => model.UserName)
                    </div>

                    <div class="display-label">
                        @Html.LabelFor(model => model.Passcode)
                    </div>
                    <div class="display-field">
                        @Html.DisplayFor(model => model.Passcode)
                    </div>
                </fieldset>
                <p>
                    @Html.ActionLink("Edit", "Edit", new { id = Model.ID }) |
                    @Html.ActionLink("Back to List", "Index")
                </p>
            </div>
        </div>
    </div>
</div>
