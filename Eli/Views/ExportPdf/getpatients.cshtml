@model IEnumerable<Eli.Models.tblTherapist>
    @using Eli.Models;
    @using System;
    @using System.Collections.Generic;

    <h2>GetPersons</h2>

    <table class="table">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.UserName)
            </th>

        </tr>

        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.UserName)
                </td>

            </tr>
        }

    </table>
