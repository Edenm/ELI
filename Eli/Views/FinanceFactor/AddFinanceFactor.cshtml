@model IEnumerable<Eli.Models.tblFinancingFactor>

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <title></title>
    <meta name="viewport" content="width=device-width" />
    <script src="~/Scripts/DataTable/dataTables.keyTable.js"></script>
    <script src="~/Scripts/DataTable/jquery.js"></script>
    <script src="~/Scripts/DataTable/jquery.dataTables.js"></script>
    <link href="~/Scripts/DataTable/dataTables.keyTable.css" rel="stylesheet" />
    <link href="~/Scripts/DataTable/jquery.dataTables.css" rel="stylesheet" />
    <link href="~/Content/ModalSize.css" rel="stylesheet" />
    <meta name="viewport" content="width=device-width, initial-scale=1">
    @Styles.Render("~/Content/bootstrapcss")
    @Styles.Render("~/Content/themes/base/css")


</head>
<body>
    <h4 align="center">גורם מממן</h4>
    <h4 align="center">@Model.Last().Name</h4>
    <h4 align="center">נוסף בהצלחה</h4>

    <h4 align="right">:חיפוש</h4>

    <table id="myTable" class="display nowrap" cellspacing="0" width="100%">
        <thead>
            <tr>
                <th>מייל איש קשר</th>
                <th>טלפון איש קשר</th>
                <th>שם איש קשר</th>

                <th>סוג גורם</th>
                <th>שם גורם</th>
                <th>מס גורם</th>

            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>

                    <td> @item.ContactMail </td>
                    <td> @item.ContcatPhoneNumber </td>
                    <td> @item.ContactName </td>

                    <td> @item.FinancingFactorType </td>
                    <td> @item.Name </td>
                    <td> @item.FinancingFactorNumber </td>
                </tr>
            }
        </tbody>
    </table>



    <input type="button" title="Delete" value="הוסף גורם מממן חדש" onclick="location.href='@Url.Action("NewFinanceFactor", "FinanceFactor")'" />





</body>


<script>


    var table = $('#myTable').DataTable();
    new $.fn.dataTable.KeyTable(table);

</script>





<script>
    function PatientDetails(id) {



        window.location = ' @Url.Action("PatientDetails", "Patient", new { id ="222222222" })';

    }

</script>







</html>


